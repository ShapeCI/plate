{
  "ignoreChanges": [
    "**test**",
    "**.spec**",
    "**/*.md",
    "**/*stories*",
    "**template**",
    "**fixture**"
  ],
  "includeMergedTags": true,
  "npmClient": "yarn",
  "command": {
    "publish": {
      "registry": "https://npm.pkg.github.com/shapeci"
    }
  },
  "packages": [
    "packages/**",
    "docs"
  ],
  "useWorkspaces": true,
  "version": "1.0.2"
}
